<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="Timeline">
        <attr name="bulletIcon" format="reference" />
        <attr name="bulletIconTint" format="color" />
        <attr name="bulletSize" format="dimension" />
        <attr name="bulletIconPadding" format="dimension" />
        <attr name="bulletCornerRadius" format="dimension" />
        <attr name="bulletBackground" format="color|reference" />
        <attr name="bulletType" format="enum">
            <enum name="icon" value="1" />
            <enum name="square" value="2" />
            <enum name="circle" value="3" />
            <enum name="round" value="4" />
        </attr>
        <attr name="bulletGravity" format="enum">
            <enum name="top" value="1" />
            <enum name="center" value="2" />
            <enum name="bottom" value="3" />
        </attr>
        <attr name="lineWidth" format="dimension" />
        <attr name="lineColor" format="color" />
        <attr name="linePadding" format="dimension" />
        <attr name="lineDashSize" format="dimension" />
        <attr name="lineDashGap" format="dimension" />
        <attr name="lineType" format="enum">
            <enum name="solid" value="1" />
            <enum name="dashed" value="2" />
            <enum name="dotted" value="3" />
        </attr>
        <attr name="lineVisibility" format="enum">
            <enum name="both" value="1" />
            <enum name="top" value="2" />
            <enum name="bottom" value="3" />
            <enum name="none" value="4" />
        </attr>
    </declare-styleable>
</resources>